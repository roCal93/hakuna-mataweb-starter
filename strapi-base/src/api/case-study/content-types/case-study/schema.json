{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case Study",
    "description": "Portfolio project or case study"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "identifier": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "client_display": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "client_is_anonymous": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "summary": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "context": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "objective": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "source_language": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "target_language": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "volume": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "volume_unit": {
      "type": "enumeration",
      "enum": ["words", "pages", "minutes", "hours"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "deadline": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "turnaround_days": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "constraints": {
      "type": "component",
      "repeatable": false,
      "component": "shared.project-constraints",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "results": {
      "type": "component",
      "repeatable": false,
      "component": "shared.project-results",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "domains": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::domain.domain",
      "inversedBy": "case_studies"
    },
    "content_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::content-type-taxonomy.content-type-taxonomy",
      "inversedBy": "case_studies"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "case_studies"
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "order": {
      "type": "integer",
      "default": 0,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "deliverables": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["files", "images", "videos"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
